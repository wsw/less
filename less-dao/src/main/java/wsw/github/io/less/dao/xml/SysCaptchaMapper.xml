<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wsw.github.io.less.dao.mapper.SysCaptchaMapper">
    <select id="findByUuidAndCode" parameterType="wsw.github.io.less.dao.entity.SysCaptcha" resultType="wsw.github.io.less.dao.entity.SysCaptcha">
      SELECT * FROM sys_captcha WHERE uuid=#{uuid} AND code=#{code};
    </select>
</mapper>
